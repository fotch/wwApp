        <script src="{{ asset("/bundles/framework/js/components/tabs.js") }}"></script>
        <script src="{{ asset("/bundles/framework/js/components/matchheight.js") }}"></script>
        <script src="{{ asset("/bundles/framework/js/components/jvectormap.js") }}"></script>
        <script src="{{ asset("/bundles/framework/js/components/peity.js") }}"></script>
        {% block javascripts %}

        {% endblock %}
        {% block customJs %}{% endblock %}
        <script type="text/javascript">
            $(document).ready(function($) {
                Site.run();
            });
        </script>
